{
  "manifest_version": 2,

  "name": "YAHNES",
  "description": "Yet Another Hacker News Extension Suite.",
  "version": "0.1.4",
  "icons": { "128": "./images/logo.png" },
  "permissions": [
    "activeTab"
  ],
  "page_action": {},

  "content_scripts": [
    {
      "matches": [
        "http://news.ycombinator.com/*",
        "https://news.ycombinator.com/*"
      ],
      "css": ["yahnes.css"],
      "js": ["jquery-2.1.1.min.js", "content.js"],
      "run_at": "document_end"
    }
  ]
}